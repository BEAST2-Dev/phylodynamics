<package name='phylodynamics' version='1.4.0'>
	<depends on='BEAST.base' atleast='2.7.0'/>
	<depends on='BDSKY' atleast='1.5.0'/>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="phylodynamics.BDSIR"/>
        <provider classname="phylodynamics.epidemiology.CoalescentSimulator"/>
        <provider classname="phylodynamics.epidemiology.CoalescentSimulatorBasic"/>
        <provider classname="phylodynamics.epidemiology.CompoundSIROperator"/>
        <provider classname="phylodynamics.epidemiology.DeterministicCoalescentSIR"/>
        <provider classname="phylodynamics.epidemiology.HybridSEIREpidemic"/>
        <provider classname="phylodynamics.epidemiology.HybridTrajectorySingleLocationSimulator"/>
        <provider classname="phylodynamics.epidemiology.SeasonalSIRSEpidemic"/>
        <provider classname="phylodynamics.epidemiology.SIR_Initializer"/>
        <provider classname="phylodynamics.epidemiology.SIRPopulationFunction"/>
        <provider classname="phylodynamics.epidemiology.StochasticCoalescent"/>
        <provider classname="phylodynamics.epidemiology.StochasticCoalescentSIR"/>
        <provider classname="phylodynamics.epidemiology.Volz2009TreeDistribution"/>
        <provider classname="phylodynamics.evolution.operators.MultiScaleOperator"/>
        <provider classname="phylodynamics.evolution.operators.ScaleOffsetOperator"/>
        <provider classname="phylodynamics.evolution.tree.coalescent.StochasticCoalescentSkyline"/>
    </service>

</package>
